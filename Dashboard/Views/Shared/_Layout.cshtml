<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Dashboard</title>
    <script type="importmap"></script>
    <link rel="stylesheet" href="@ResoursePathHelper.GetRightPath("~/lib/bootstrap/dist/css/bootstrap.min.css")" />
    <link rel="stylesheet" href="@ResoursePathHelper.GetRightPath("~/css/site.css")" asp-append-version="true" /> 
    <link rel="stylesheet" href="@ResoursePathHelper.GetRightPath("~/Dashboard.styles.css")" asp-append-version="true" />


</head>
<body>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
